{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start:server:dev": "NODE_ENV=development node server/index.js",
    "start:client:dev": "NODE_ENV=development vite",
    "dev": "concurrently \"npm run start:server:dev\" \"npm run start:client:dev\"",
    "start:server:prod": "NODE_ENV=production node server/index.js",
    "start:client:prod": "NODE_ENV=production vite",
    "prod": "concurrently \"npm run start:server:prod\" \"npm run start:client:prod\"",
    "build": "NODE_ENV=production vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "mocha",
    "test:watch": "mocha --watch",
    "test:google-play:id": "mocha server/googlePlayScraper.test.js -g 'extractGooglePlayId'",
    "test:google-play:details": "mocha server/googlePlayScraper.test.js -g 'fetchAppDetails'",
    "test:google-play:reviews": "mocha server/googlePlayScraper.test.js -g 'getAppReviews'",
    "test:google-play:process-url": "mocha server/googlePlayScraper.test.js -g 'processGooglePlayUrl'",
    "test:app-store:id": "mocha server/appStoreScraper.test.js -g 'extractAppStoreId'",
    "test:app-store:details": "mocha server/appStoreScraper.test.js -g 'getAppDetails'",
    "test:app-store:reviews": "mocha server/appStoreScraper.test.js -g 'getAppReviews'",
    "test:app-store:process-url": "mocha server/appStoreScraper.test.js -g 'processAppStoreUrl'"
  },
  "dependencies": {
    "@stripe/react-stripe-js": "^3.1.1",
    "@stripe/stripe-js": "^5.6.0",
    "app-store-scraper": "^0.18.0",
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.3",
    "google-play-scraper": "^10.0.1",
    "lucide-react": "^0.344.0",
    "openai": "^4.28.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "stripe": "^17.7.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@babel/preset-env": "^7.26.7",
    "@eslint/js": "^9.9.1",
    "@tailwindcss/typography": "^0.5.10",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "axios-mock-adapter": "^2.1.0",
    "chai": "^5.1.2",
    "concurrently": "^8.2.2",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "mocha": "^11.1.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vite-plugin-csp": "^1.1.2"
  },
  "mocha": {
    "spec": "server/**/*.test.js",
    "require": [
      "chai/register-expect.js"
    ]
  }
}
